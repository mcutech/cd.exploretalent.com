{
  "name": "pixel-admin-base",
  "version": "0.0.0",
  "authors": [
    "Ryan Eballar <ryeballar@gmail.com>"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "pixel-admin-base-assets": "git@gitlab.com:utech/pixel-admin-base-assets.git#v0.0.5",
    "bootstrap-tagsinput": "~0.5.0",
    "moment": "~2.10.6",
    "bootstrap-multiselect": "~0.9.13"
  },
  "overrides": {
    "slimScroll": {
      "main": [
        "jquery.slimscroll.min.js"
      ]
    },
    "bootstrap-sass-official": {
      "main": [
        "./assets/javascripts/bootstrap/transition.js",
        "./assets/javascripts/bootstrap/alert.js",
        "./assets/javascripts/bootstrap/button.js",
        "./assets/javascripts/bootstrap/carousel.js",
        "./assets/javascripts/bootstrap/collapse.js",
        "./assets/javascripts/bootstrap/dropdown.js",
        "./assets/javascripts/bootstrap/tooltip.js",
        "./assets/javascripts/bootstrap/popover.js",
        "./assets/javascripts/bootstrap/scrollspy.js",
        "./assets/javascripts/bootstrap/tab.js",
        "./assets/javascripts/bootstrap/affix.js",
        "./assets/stylesheets/_bootstrap.scss"
      ]
    },
    "bootstrap-select": {
      "main": [
        "dist/css/bootstrap-select.css",
        "dist/js/bootstrap-select.js"
      ]
    },
    "bootstrap-datepicker": {
      "main": "js/bootstrap-datepicker.js"
    },
    "x-editable": {
      "main": [
        "dist/bootstrap3-editable/js/bootstrap-editable.js",
        "dist/inputs-text/address/address.js",
        "dist/inputs-text/typeaheadjs/lib/typeahead.js",
        "dist/inputs-text/typeaheadjs/typeahead.js"
      ]
    },
    "bootstrap-markdown": {
      "main": "js/bootstrap-markdown.js"
    },
    "evilstreak-markdown-js": {
      "main": "lib/markdown.js"
    },
    "datatables": {
      "main": [
        "media/js/jquery.dataTables.js",
        "media/images/*.png"
      ]
    },
    "datatables-bootstrap": {
      "main": [
        "integration/bootstrap/1/dataTables.bootstrap.js"
      ]
    },
    "flot": {
      "main": [
        "jquery.flot.js",
        "jquery.flot.pie.js"
      ]
    },
    "jquery-validation": {
      "main": [
        "jquery.validate.js",
        "additional-methods.js"
      ]
    },
    "select2": {
      "main": "select2.js"
    },
    "summernote": {
      "main": "dist/summernote.js"
    },
    "bootstrap-datepaginator": {
      "main": "dist/bootstrap-datepaginator.min.js"
    },
    "bootstrap-timepicker": {
      "main": "js/bootstrap-timepicker.min.js"
    },
    "dropzone": {
      "main": [
        "dist/dropzone-amd-module.js",
        "dist/dropzone.js"
      ]
    },
    "growl": {
      "main": "javascripts/jquery.growl.js"
    },
    "jquery-minicolors": {
      "main": "./jquery.minicolors.js"
    },
    "jquery.sparkline": {
      "main": "index.js"
    },
    "morris.js": {
      "main": "morris.js",
      "dependencies": {
        "jquery": ">= 2.1.0",
        "raphael": ">= 2.0"
      }
    },
    "jquery.easy-pie-chart": {
      "dependencies": {
        "jquery": ">=1.9.0"
      }
    }
  },
  "resolutions": {
    "bootstrap": "~3",
    "jquery": ">= 2.1.0",
    "moment": "~2.10.6",
    "bootstrap-datepicker": "~1.2.0",
    "components-font-awesome": "~4.2.0"
  }
}
